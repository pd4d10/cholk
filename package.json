    "karma-jasmine": "^1.1.0",
    "karma-rollup-preprocessor": "^4.0.1",
    "karma-safari-launcher": "^1.0.0",
    "prettier": "^1.5.3",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
